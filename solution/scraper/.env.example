# Configuración de base de datos PostgreSQL
POSTGRES_USER=postgres
POSTGRES_PASSWORD=123456789
POSTGRES_DB=links_db
POSTGRES_HOST=localhost
POSTGRES_PORT=5432

# Configuración CORS
# Para múltiples orígenes, separa con comas
# Ejemplos:
# CORS_ORIGINS=http://localhost:3000,http://localhost:8080,https://mydomain.com
# CORS_ORIGINS=*  # Para permitir todos los orígenes (solo desarrollo)
CORS_ORIGINS=http://localhost:3000,http://localhost:8080

# Variables de entorno Docker
DOCKER_ENV=true
